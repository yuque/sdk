{
  "name": "@yuque/sdk",
  "version": "1.1.1",
  "description": "Node SDK for yuque",
  "main": "lib/index.js",
  "dependencies": {
    "debug": "^4.1.1",
    "extend2": "^1.0.0",
    "urllib": "^2.33.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "egg-bin": "^5.1.1",
    "egg-ci": "^1.8.0",
    "eslint": "^8.3.0",
    "eslint-config-egg": "^12.0.0",
    "typescript": "^4.7.4",
    "@types/node": "^14.18.21"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
